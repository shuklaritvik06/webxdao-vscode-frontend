{
  "name": "webxdao",
  "displayName": "WebXDAO",
  "description": "WebXDAO Helper, a vs code extension to help our contributors",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:react-webview.start"
  ],
  "publisher": "shuklaritvik06",
  "main": "./build/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-webview.start",
        "title": "Start React Webview",
        "category": "React"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --config webpack.config.js && npm run babel",
    "babel": "babel src/**/*.ts --out-file ./build/extension.js",
    "build": "webpack --config webpack.config.js && npm run babel",
    "start": "react-scripts start",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "postcss": "^8.4.20",
    "react-scripts": "^5.0.1",
    "rewire": "^6.0.0",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "axios": "^1.2.1",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.14",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolve-cwd": "^3.0.0",
    "vscode": "^1.1.37"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
